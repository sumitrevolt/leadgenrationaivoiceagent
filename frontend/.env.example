# AuraLeads Frontend Production Environment
# Copy this file to .env.production and update with your values

# API Configuration
VITE_API_URL=https://api.auraleads.ai/api/v1
VITE_WS_URL=wss://api.auraleads.ai/ws

# Authentication
VITE_AUTH_DOMAIN=auth.auraleads.ai
VITE_AUTH_CLIENT_ID=your-auth-client-id

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_VOICE_AGENTS=true
VITE_ENABLE_AI_CHAT=true

# Sentry Error Tracking
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Google Analytics
VITE_GA_TRACKING_ID=G-XXXXXXXXXX

# Environment
VITE_NODE_ENV=production
